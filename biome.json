{
	"$schema": "https://biomejs.dev/schemas/1.9.2/schema.json",
	"vcs": {
	  "enabled": false,
	  "clientKind": "git",
	  "useIgnoreFile": false
	},
	"files": {
	  "ignoreUnknown": false,
	  "ignore": []
	},
	"formatter": {
	  "enabled": true,
	  "indentStyle": "space",
	  "indentWidth": 2
	},
	"organizeImports": {
	  "enabled": true
	},
	"linter": {
	  "enabled": true,
	  "rules": {
		"recommended": true
	  }
	},
	"javascript": {
	  "formatter": {
		"arrowParentheses": "always",
		"quoteStyle": "single",
		"semicolons": "asNeeded",
		"trailingCommas": "es5",
		"lineWidth": 120
	  }
	},
	"json": {
	  "formatter": {
		"lineWidth": 20
	  }
	}
  }
  